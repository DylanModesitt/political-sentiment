\documentclass{article}
\usepackage{amsmath}
\begin{document}
  \begin{align}
    y &= (x_{1},x_{2},\cdots, x_{N})
        \begin{pmatrix}
          \begin{bmatrix}
           ax_{0} + bx_{1} \\           
           \vdots \\
           ax_{n-1}+bx_{n}
          \end{bmatrix} -
          \begin{bmatrix}
           z_{1} \\
           \vdots \\
           z_{n}
         \end{bmatrix}
    \end{pmatrix}
  \end{align}
\end{document}
